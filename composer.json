{
    "name": "drewlabs/laravexists",
    "description": "Library provides existance rule implementation for laravel/lumen based projects",
    "keywords": [
        "laravel",
        "validation",
        "exists"
    ],
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": ">=6.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\LaravExists\\": "src/"
        },
        "files": [
            "src/proxies.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\LaravExists\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "illuminate/validation": "^10.0|^11.0",
        "psr/http-message": "^1.0|^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
